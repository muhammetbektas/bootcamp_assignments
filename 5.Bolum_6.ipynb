   "source": [
    "SELECT COUNT(*) as Miktar,\n",
    "\n",
    "CASE\n",
    "\n",
    "    WHEN Miktar>20 THEN 'büyük'\n",
    "\n",
    "    WHEN Miktar>10 THEN 'orta'\n",
    "\n",
    "    ELSE 'küçük' \n",
    "\n",
    "END AS Sınıf\n",
    "\n",
    "FROM(SELECT country AS Ulke, COUNT(*) Miktar\n",
    "\n",
    "FROM customers\n",
    "\n",
    "GROUP BY country\n",
    "\n",
    "ORDER BY Miktar DESC) class\n",
    "\n",
    "GROUP BY Sınıf;"
   ]
